%function [GOPDecodedFrames]=main_decoder(frameSize,nOfFrames,blockSize,noframes,dict)

noframes=3;
blockSize=8;
frameSize=[288 352];
 q_mtx =     [16 11 10 16 24 40 51 61; 
             12 12 14 19 26 58 60 55;
             14 13 16 24 40 57 69 56; 
             14 17 22 29 51 87 80 62;
             18 22 37 56 68 109 103 77;
             24 35 55 64 81 104 113 92;
             49 64 78 87 103 121 120 101;
             72 92 95 98 112 100 103 99];
GOPDecodedFrames=zeros([noframes frameSize]);
bitstreamIdx=1;
i=1;
[quantizedFrames,currBitIdx]=iRLC(bitstream,blockSize,frameSize,dict,bitstreamIdx,noframes);
while i<noframes
    rev_quantized_frames(i,:,:)=reverse_block_quantizer(quantizedFrames(i,:,:),blockSize,q_mtx);
    GOPDecodedFrames(i,:,:)=block_idct_frame(rev_quantized_frame,blockSize);
    i=i+1;
end

%end
